{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "id": {
      "type": "number"
    },
    "id_exercise": {
      "type": "number"
    },
    "object": {
      "type": "string"
    },

    "service_number": {
      "type": "string"
    },
    "cp_number": {
      "type": "string"
    },
    "date_cp": {
      "type": "date"
    },
    "comment": {
      "type": "string"
    },
    "submitted_to_cp": {
      "type": "boolean"
    }
  },
  "required": [ "id_exercise", "object"]
}
